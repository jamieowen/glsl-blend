{
  "name": "glsl-blend",
  "version": "1.0.3",
  "description": "glsl photoshop blend modes. glslify formatted.",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "snowpack dev",
    "build": "snowpack build",
    "__start": "budo demo/demo-three.js -- -t glslify",
    "__bundle": "browserify demo/demo-three.js -t glslify | uglifyjs -cm > demo/demo-three.min.js",
    "gh-pages": "yarn build && cp public/.nojekyll build && gh-pages -t -d build",
    "export": "ts-node scripts/export.ts",
    "ex": "node --loader ts-node/esm ./scripts/export.ts",
    "gen": "node --experimental-specifier-resolution=node ./ast-generate.js",
    "tsc": "tsc"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:jamieowen/glsl-blend.git"
  },
  "author": "Jamie Owen",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jamieowen/glsl-blend/issues"
  },
  "homepage": "https://github.com/jamieowen/glsl-blend",
  "devDependencies": {
    "@thi.ng/api": "^8.3.7",
    "@thi.ng/shader-ast": "^0.12.10",
    "gh-pages": "^4.0.0",
    "typescript": "^4.7.4"
  },
  "exports": {
    "./ast": "./ast/index.js"
  }
}
